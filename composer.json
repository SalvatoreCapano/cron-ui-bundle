{
    "name": "salvatorecapano/cron-ui-bundle",
    "description": "Symfony bundle providing a UI for managing CronBundle jobs.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Salvatore Capano"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^6.0",
        "cron/cron-bundle": "^3.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Salvatore\\CronUiBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    }
}